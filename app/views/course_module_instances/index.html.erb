<p id="notice"><%= notice %></p>

<h1>Listing Course Module Instances</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Course module</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_module_instances.each do |course_module_instance| %>
      <tr>
        <td><%= course_module_instance.course_id %></td>
        <td><%= course_module_instance.course_module_id %></td>
        <td><%= course_module_instance.user_id %></td>
        <td><%= link_to 'Show', course_module_instance %></td>
        <td><%= link_to 'Edit', edit_course_module_instance_path(course_module_instance) %></td>
        <td><%= link_to 'Destroy', course_module_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course module instance', new_course_module_instance_path %>
