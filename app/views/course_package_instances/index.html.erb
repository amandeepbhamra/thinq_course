<p id="notice"><%= notice %></p>

<h1>Listing Course Package Instances</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Course package</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_package_instances.each do |course_package_instance| %>
      <tr>
        <td><%= course_package_instance.course_id %></td>
        <td><%= course_package_instance.course_package_id %></td>
        <td><%= course_package_instance.user_id %></td>
        <td><%= link_to 'Show', course_package_instance %></td>
        <td><%= link_to 'Edit', edit_course_package_instance_path(course_package_instance) %></td>
        <td><%= link_to 'Destroy', course_package_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course package instance', new_course_package_instance_path %>
